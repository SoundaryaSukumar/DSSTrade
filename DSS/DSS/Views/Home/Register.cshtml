});
$("#tokenSys").click(function(){
	$("#mempackId").css('display', 'none');
	$("#pay_typeId").css('display', 'none');
	$("#tokensysId").css('display', 'block');
	$("#epinId").prop("required",true);
});

$("#online").click(function(){
	$("#offlineId").css('display', 'none');
	$("#payslipId").prop("required",false);
	$("#onlineId").css('display', 'block');
});
$("#offline").click(function(){
	$("#onlineId").css('display', 'none');
	$("#offlineId").css('display', 'block');
