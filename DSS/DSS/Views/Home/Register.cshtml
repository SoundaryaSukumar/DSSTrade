<script>function usrplacement(spnsrid) {
	if(spnsrid!="") {
		$.ajax({
			type: 'POST',
			url: "admin/ajax.php",
			data: {spnsr_plc:spnsrid},
			dataType: "text",
			success: function(result) {
				var result=JSON.parse(result);
				if(result=="0") {
					alert('Invalid Sponsor ID');
					document.getElementById("sid").value='';
				}
				else {
					document.getElementById("sname").value=result.name;
				}
			}
		});
	}
}

$("#memPack").click(function(){
	$("#tokensysId").css('display', 'none');
	$("#mempackId").css('display', 'block');
